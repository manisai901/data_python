1. docker compose up airflow-init
2. docker compose up -d
3. docker compose logs -f
4. docker compose down
5. docker compose down --volumes --rmi all
6. docker compose ps
 docker ps --all
 --- to create .env file with current user id ---
 echo -e "AIRFLOW_UID=$(id -u)\nAIRFLOW_GID=0" > .env
  docker compose run airflow-worker pip install -r requirements.txt
   pip install -r requirements.txt
 mkdir ./data ./scripts ./logs ./plugins ./dags
--- to create required sub folders ---
  mkdir -p dags logs plugins data/gcs_mock data/bigquery_mock

--- to restart webserver ---
 docker compose restart airflow-webserver


 --- to trigger perticular dag ---
 docker compose exec airflow-webserver airflow dags trigger youtube_etl_full

 --- to pause perticular dag ---
  docker compose exec airflow-webserver airflow dags unpause youtube_etl_full
    --- to unpause perticular dag ---
    docker compose exec airflow-webserver airflow dags pause youtube_etl_full

--- to see the list of dags ---
  docker compose exec airflow-webserver airflow dags list-import-errors


