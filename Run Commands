19. docker compose top
20. docker compose version
21. docker compose help
22. docker compose cp <source_path> <destination_path>
23. docker compose scale <service_name>=<number_of_instances>
24. docker compose port <service_name> <private_port>
25. docker compose events
26. docker compose exec <service_name> env
27. docker compose exec <service_name> ls /path/to/directory
28. docker compose exec <service_name> cat /path/to/file
29. docker compose exec <service_name> tail -f /path/to/logfile
30. docker compose exec <service_name> top
31. docker compose exec <service_name> df -h
32. docker compose exec <service_name> free -m
33. docker compose exec <service_name> ps aux
34. docker compose exec <service_name> netstat -tuln
35. docker compose exec <service_name> curl http://localhost:8080
36. docker compose exec <service_name> ping google.com
37. docker compose exec <service_name> apt-get update
38. docker compose exec <service_name> apt-get install <package_name>
39. docker compose exec <service_name> yum update
40. docker compose exec <service_name> yum install <package_name>